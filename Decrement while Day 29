import java.util.Scanner;
 class Main {
    
    public static void main(String[] args) {
        Scanner input = new Scanner (System.in);
        System.out.print("Hitung mundur dari angka : ");
        int a = input.nextInt();
        while(1<=a){
            a--;
            System.out.print(a+" ");
            
        }
            
        System.out.println();
        
    }
 }
        
        
 
    
    
