package looping;
import java.util.Scanner;
public class Looping {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.print("Masukan inputan : ");
        int jml = input.nextInt();
        for (int i = 1; i <= jml; i++) {
            for (int j = 1; j < i; j++) {
                System.out.print("o  ");
               }
            System.out.println("o    ");
            
        } 
    }
    
}
