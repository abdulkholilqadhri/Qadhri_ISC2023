package main;

public class Main {
    
    public static void main(String[] args) {
        int a = 1;
        int b = 2;
        int c = 0;
        System.out.println("Nilai a = "+a);
        System.out.println("Nilai b = "+b);
        System.out.println("======setelah ditukar=====");
         
         c = a;
        a = b;
        b = c;
        System.out.println("Nilai b = "+b);
        System.out.print("Nilai a = "+a);
        
    }
    
}
