package main;
import java.util.*;

public class Main {
    
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int a = input.nextInt();
        int b = input.nextInt();
        belajar(a,b);
        
    }
    private static void belajar(int panjang, int lebar){
        for(int i = 1 ; i<= lebar;i++){
            for(int j = 1; j<= panjang;j++){
                System.out.print("* ");
            }
            System.out.println("");
        }
    }
}

    
