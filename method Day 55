package belajar_1;

public class Belajar_1 {
    //prosedur
    public static void isc(){
        String kata = "Cara mendeklarasikan PROSEDUR";
        System.out.println(kata);
    }
    //fungsi
    public static String days(){
        String kalimat = "Cara mendeklarasikan FUNGSI";
        return kalimat;
    }
   
     public static void main(String[] args) {
        isc();
         System.out.println(days());
    }
}
