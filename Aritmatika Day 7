
public class Main {
    
    public static void main(String[] args) {
        double a, b, c, d;
        a = 14;
        b = 28;
        c = 13;
        d = 50;
        
        System.out.println(a + b);
        System.out.println(b - c);
        System.out.println(c / d);
        System.out.println(a * d);
        System.out.println(b % c);
    }
    
}
